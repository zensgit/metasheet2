{
  "title": "Phase 5 SLO Dashboard",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "HTTP Success Rate (5m)",
      "targets": [{"expr": "metasheet:http_success_rate:5m"}]
    },
    {
      "type": "stat",
      "title": "Cache Hit Rate (10m)",
      "targets": [{"expr": "metasheet:cache_hit_rate:10m"}]
    },
    {
      "type": "stat",
      "title": "Fallback Effective Ratio (10m)",
      "targets": [{"expr": "metasheet:fallback_effective_ratio:10m"}]
    },
    {
      "type": "stat",
      "title": "Plugin Reload p95 (5m)",
      "targets": [{"expr": "metasheet:plugin_reload_p95:5m"}]
    },
    {
      "type": "stat",
      "title": "Plugin Reload p99 (5m)",
      "targets": [{"expr": "metasheet:plugin_reload_p99:5m"}]
    },
    {
      "type": "stat",
      "title": "Snapshot Create p95 (5m)",
      "targets": [{"expr": "label_replace(metasheet:snapshot_op_p95:5m, \"operation\", \"create\", \"operation\", \".*\")"}]
    },
    {
      "type": "stat",
      "title": "Snapshot Restore p95 (5m)",
      "targets": [{"expr": "label_replace(metasheet:snapshot_op_p95:5m, \"operation\", \"restore\", \"operation\", \".*\")"}]
    }
  ]
}
